      <artifactId>slf4j-simple</artifactId>
      <version>1.7.5</version>
      <scope>provided</scope>
    </dependency>
  </dependencies>
  <properties>
    <spring.batch.job.enabled>false</spring.batch.job.enabled>
    <java.version>1.8</java.version>
  </properties>
</project>
